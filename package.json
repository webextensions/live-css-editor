{
  "name": "magic-css",
  "version": "8.22.0",
  "scripts": {
    "install:legacy-peer-deps": "npm install --legacy-peer-deps",
    "version": "./scripts/build-and-release/prepare-version/prepare-version.sh",
    "postversion": "git push --follow-tags",
    "postinstall": "npm run copy-files-from-to",
    "copy-files-from-to": "         copy-files-from-to",
    "copy-files-from-to:outdated": "copy-files-from-to --outdated",
    "copy-files-from-to:dev-mode": "copy-files-from-to --mode dev",
    "lint": "eslint .",
    "all-is-well": "./scripts/all-is-well.sh",
    "test": "npm run all-is-well",
    "test:puppeteer": "cd tests ; npm test",
    "clean": "cd ./extension-dist/ && git clean -Xdf",
    "start": "      npm run start:build",
    "start:build": "npm run start:build:dev",
    "start:build:dev": "            cd extension/ ;                                         webpack --mode development --config ./webpack/webpack.config.js --env build.buildFor='development' --env build.watch",
    "start:build:dev:inspect-brk": "cd extension/ ; node --inspect-brk ../node_modules/.bin/webpack --mode development --config ./webpack/webpack.config.js --env build.buildFor='development' --env build.watch",
    "start:build:publish": "        cd extension/ ; node               ../node_modules/.bin/webpack --mode development --config ./webpack/webpack.config.js --env build.buildFor='publish'",
    "start:build:publish:watch": "  cd extension/ ;                                         webpack --mode development --config ./webpack/webpack.config.js --env build.buildFor='publish'     --env build.watch",
    "manifest-generator": "node ./extension/manifest-generator.mjs",
    "zip:extension:chrome": " node ./zip-extension.mjs chrome",
    "zip:extension:edge": "   node ./zip-extension.mjs edge",
    "zip:extension:firefox": "node ./zip-extension.mjs firefox",
    "zip:extension:opera": "  node ./zip-extension.mjs opera",
    "zip:extension": "npm run zip:extension:chrome && npm run zip:extension:edge && npm run zip:extension:firefox && npm run zip:extension:opera",
    "publish:build-and-zip": "npm run clean && npm run start:build:publish && npm run zip:extension",
    "get-latest-reviews": "node ./scripts/recipes/get-latest-reviews.mjs"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@react-hook/window-size": "^3.1.1",
    "babel-loader": "^9.1.2",
    "classnames": "^2.3.2",
    "codemirror": "=5.58.2",
    "crypto-js": "^4.1.1",
    "css-loader": "^6.7.3",
    "emmetio-codemirror-plugin-webextensions": "^0.3.7",
    "helpmate": "=1.5.4",
    "helpmate-css": "=0.1.1",
    "immer": "^10.0.1",
    "jquery": "^3.6.4",
    "mini-css-extract-plugin": "^2.7.5",
    "mixpanel-browser": "webextensions/mixpanel-js-mv3#fetch-support-for-mv3-extensions",
    "oauth-1.0a": "^2.2.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-command-palette": "=0.22.1",
    "react-dom": "^18.2.0",
    "react-joyride": "=2.5.3",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.2",
    "react-roving-tabindex": "^3.2.0",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "rooks": "^7.11.2",
    "socket.io": "=2.5.0",
    "toastr": "^2.1.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@rbarilani/remove-source-map-url-webpack-plugin": "=1.1.0",
    "archiver": "^5.3.1",
    "chalk": "^5.2.0",
    "copy-files-from-to": "^3.8.0",
    "copy-webpack-plugin": "^11.0.0",
    "cp-file": "^10.0.0",
    "del": "^7.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "git-hooks": "^1.1.10",
    "jsonfile": "^6.1.0",
    "node-notifier": "^10.0.1",
    "note-down": "^0.3.1",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1"
  }
}
